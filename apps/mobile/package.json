{
  "name": "petspark-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:run": "vitest run",
    "build:eas": "eas build --platform all",
    "build:eas:ios": "eas build --platform ios",
    "build:eas:android": "eas build --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android"
  },
  "dependencies": {
    "@petspark/shared": "workspace:*",
    "@react-navigation/bottom-tabs": "^7.0.12",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.1.7",
    "expo": "~51.0.34",
    "expo-constants": "~16.0.2",
    "expo-linking": "~5.0.1",
    "expo-status-bar": "~1.13.1",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-web": "~0.19.12"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/js": "^9.21.0",
    "@types/react": "^18.3.12",
    "@types/react-native": "^0.76.0",
    "@vitest/coverage-v8": "^2.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.28.0",
    "tailwindcss": "^3.4.1",
    "typescript-eslint": "^8.38.0",
    "typescript": "~5.7.2",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
