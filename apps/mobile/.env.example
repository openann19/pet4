# Mobile Client Environment Variables
# Copy this file to .env and adjust for local development

# Core API configuration (must be HTTPS for production builds)
EXPO_PUBLIC_API_URL=https://api.petspark.app/api

# Logging
EXPO_PUBLIC_LOG_LEVEL=INFO

# Feature flags
EXPO_PUBLIC_ENABLE_PAYMENTS=true
EXPO_PUBLIC_ENABLE_KYC=true
EXPO_PUBLIC_ENABLE_LIVE_STREAMING=true

# Analytics / Telemetry (public keys only)
EXPO_PUBLIC_SENTRY_DSN=
EXPO_PUBLIC_SENTRY_TRACES_SAMPLE_RATE=0.1
EXPO_PUBLIC_ANALYTICS_WRITE_KEY=

# Authentication configuration
EXPO_PUBLIC_AUTH_DOMAIN=auth.petspark.app
EXPO_PUBLIC_AUTH_CLIENT_ID=

# Misc
EXPO_PUBLIC_APP_ENV=production
