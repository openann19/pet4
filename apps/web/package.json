    "fast-glob": "^3.3.2",
    "globals": "^16.0.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "postcss-safe-parser": "^7.0.1",
    "prettier": "^3.4.2",
    "pyright": "^1.1.407",
    "react-day-picker": "^9.11.1",
    "react-leaflet": "^5.0.0",
    "react-native-web": "^0.21.2",
    "recharts": "^3.4.1",
    "size-limit": "^11.1.4",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^4.1.11",
    "ts-prune": "^0.10.3",
    "tsx": "^4.19.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.38.0",
    "vaul": "^1.1.2",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^4.0.6"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "size-limit": [
    {
      "path": "dist/**/*.{js,css}",
      "limit": "500 KB"
    }
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ]
  }
}
